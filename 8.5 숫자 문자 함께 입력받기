#include <stdio.h>

void display(char cr, int lines, int width);

int main()
{
	char c;
	int rows, cols;

	while ((c=getchar()) != '\n') {
		scanf("%d %d", &rows, &cols);
		while (getchar() != '\n')continue;
		display(c, rows, cols);
		if (c == '\n')
			break;
	}
}

void display(char cr, int lines, int width)
{
	for (int i = 0; i < width; i++) {
		for (int j = 0; j < lines; j++) {
			printf("%c", cr);
		}
		printf("\n");
	}
}
